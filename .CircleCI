version: 2.1
workflows:
  test-deploy:
  jobs:
    - test
jobs:
  build:
    docker:
      - image: cimg/node:19.6.0
    steps:
      - checkout
      - node --version
      
      - run:
          name: Install dependencies
          command: npm install
      - run:
          name: Run tests
          command: npm test --coverage 
